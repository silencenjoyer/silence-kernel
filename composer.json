{
  "name": "silencenjoyer/silence-kernel",
  "type": "library",
  "description": "Silence application kernel.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Gebrich",
      "email": "an_gebrich@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Silence\\Kernel\\": ""
