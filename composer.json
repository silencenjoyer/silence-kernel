      "/Tests/"
    ]
  },
  "require": {
    "php": "^8.3.0",
    "psr/container": "^2.0",
    "psr/http-factory": "^1.0.0",
    "psr/event-dispatcher": "^1.0",

    "silencenjoyer/silence-kernel-extension": "*",
    "silencenjoyer/silence-http": "*",
    "silencenjoyer/silence-config": "*",
    "silencenjoyer/silence-error-handling": "*",
    "silencenjoyer/silence-http-parser": "*",
    "silencenjoyer/silence-event": "*",
    "silencenjoyer/silence-routing": "*",
    "silencenjoyer/silence-runtime": "*",

    "symfony/dotenv": "^7.3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3.0",

    "nyholm/psr7": "^1.8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "*",
    "symfony/var-dumper": "^7.3",
    "shipmonk/composer-dependency-analyser": "dev-master"
  }
